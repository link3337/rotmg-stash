{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "RotMG Stash",
  "version": "1.0.2",
  "identifier": "com.rotmg-stash.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "RotMG Stash",
        "width": 1600,
        "height": 900,
        "zoomHotkeysEnabled": true,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}